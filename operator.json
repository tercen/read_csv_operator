{
  "name": "CSV",
  "description": "Import a Comma-Seperated Values file or multiple CSV files in a ZIP file.",
  "tags": [
    "import"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/read_csv_operator"
  ],
  "container": "ghcr.io/tercen/read_csv_operator:1.0.1",
  "properties": [
    {
      "kind": "BooleanProperty",
      "name": "Headers",
      "defaultValue": true,
      "description": "A logical value indicating whether the file starts with a header line."
    },
    {
      "kind": "EnumeratedProperty",
      "name": "Separator",
      "defaultValue": "Comma",
      "values": [
        "Comma",
        "Tab"
      ],
      "description": "Type of the separator between values"
    },
    {
      "kind": "StringProperty",
      "name": "NA String",
      "defaultValue": "",
      "description": "A character vector of strings which are to be interpreted as NA values."
    },
    {
      "kind": "BooleanProperty",
      "name": "Force",
      "defaultValue": false,
      "description": "A logical value indicating whether the merging of different files should happen even if they do not have strictly identical columns."
    },
    {
      "kind": "BooleanProperty",
      "name": "Add namespace",
      "defaultValue": false,
      "description": "A logical value indicating whether the factors name should be prefixed by the step namespace."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Document ID",
            "type": "string",
            "description": "Document ID(s) of CSV file(s) or ZIP archive(s)",
            "ontologyMapping": "document",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [],
              "rColumns": []
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "filename",
                  "type": "string"
                },
                {
                  "kind": "Attribute",
                  "name": "rowId",
                  "type": "int32"
                },
                {
                  "kind": "Attribute",
                  "name": "filename_of_zip",
                  "type": "string"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}